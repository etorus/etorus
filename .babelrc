{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["module-resolver", {
      "alias": {
        "requests": "./src/requests",
        "modules": "./src/modules",
        "assets": "./src/assets",
        "images": "./src/assets/images",
        "app": "./src",
      }
    }],
  ]
}
